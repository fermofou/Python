def generate_magic_cube(order):
    if order % 2 == 0:
        print("Only odd-order magic cubes are supported.")
        return []

    cube = [[[0 for _ in range(order)] for _ in range(order)] for _ in range(order)]

    l, r, c = 0, order // 2, order // 2

    for num in range(1, order**3 + 1):
        cube[l][r][c] = num

        l = (l - 1 + order) % order
        c = (c - 1 + order) % order

        if cube[l][r][c] != 0:
            l = (l - 1 + order) % order
            r = (r - 1 + order) % order

        if cube[l][r][c] != 0:
            l = (l + 2) % order

    return cube

print("Magic Cube!!!")
